pipeline {
    	agent { 
		docker { 
			image 'alpine:latest' 
		}
	}
	parameters { 
		string(name: 'NAME', defaultValue: 'Mr Jenkins', description: 'Type your username:')
		string(name: 'AGE', defaultValue: '26', description: 'Type your age:')
	}
    	stages {
        	stage('build') {
            		steps {
                		echo "$NAME"
                		echo "$AGE"
            		}
        	}
    	}
}
